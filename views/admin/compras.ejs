<% include partials/headerAdmin %>

<!-- Begin Page Content -->
<div class="container-fluid">
  <!-- Content Row -->

  <form action="/admin/processOrder" method="POST">
    <div class="card shadow mb-4">
      <div class="card-header py-3">
        <h6 class="m-0 font-weight-bold text-primary">Tabela Compras</h6>
      </div>
      <div class="card-body">
        <div class="table-responsive">
          <table class="table table-bordered" id="tableMain" width="100%" cellspacing="0">
            <thead>
              <tr>
                <th>ID</th>
                <th>Document Date</th>
                <th>Seller</th>
                <th>Amount</th>
                <th>Quantity</th>
                <th>Description</th>
             
              </tr>
            </thead>
            <tbody>
              <% for(var i = 0 ; i < body.data.length ; i++){ %>
              <tr>
                <td><%= i %></td>
                <td><%= body.data[i].documentDate %></td>
                <td><%= body.data[i].sellerSupplierPartyName %></td>
                <td><%= body.data[i].amount %>â‚¬</td>
                <td><%= body.data[i].quantity %></td>
                <td><%= body.data[i].description %></td>
              </tr>
              <% } %>
            </tbody>
          </table>
  
        </div>
      </div>
    </div>
  </form>
</div>







<!-- Scroll to Top Button-->
<a class="scroll-to-top rounded" href="#page-top">
  <i class="fas fa-angle-up"></i>
</a>

